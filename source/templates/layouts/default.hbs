<!DOCTYPE html>
<!--[if IE 8]><html class="lt-ie9" lang="en"><![endif]-->
<!--[if gt IE 8]><!--><html lang="en"><!--<![endif]-->
<head>
	<meta charset="utf-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<meta name="viewport" content="width=device-width, initial-scale=1">
	{{#if site.debug}}
		<!-- while in prototype mode -->
		<meta name="robots" content="noindex, nofollow">
		<!-- /prototype mode -->
	{{else}}
		<meta name="robots" content="index, follow">
	{{/if}}
	

	{{> head-opengraph }}

	<link rel="shortcut icon" href="./favicon.ico" type="image/x-icon">
	
	<link href="https://fonts.googleapis.com/css?family=Lato:100,300,400,500,700&subset=latin" rel="stylesheet">

	<!-- build:css(./.tmp) styles/styles.min.css -->
	<link rel="stylesheet" href="styles/style.css">
	<!-- endbuild -->
</head>
<body>
	{{> body-gtm }}

	<div class="container-fluid">

		{{!--> body-header --}}

		{% body %}

		{{> body-footer }}

	</div>

	{{> bouncer }}

	<!-- build:js(./source) scripts/vendor/jquery.min.js -->
	<script src="scripts/vendor/jquery/jquery.js"></script>
	<!-- endbuild -->

	<!-- build:js(./source) scripts/vendor/modernizer-custom.min.js -->
	<script src="scripts/vendor/modernizr/modernizr-custom.js"></script>
	<!-- endbuild -->

	<!-- build:js(./source) scripts/vendor/js.cookie-2.1.3.min.js -->
	<script src="scripts/vendor/js-cookie/js.cookie-2.1.3.min.js"></script>
	<!-- endbuild -->

	<!-- build:js(./source) scripts/main.min.js -->
	<script src="scripts/main.js"></script>
	<!-- endbuild -->

</body>
</html>
